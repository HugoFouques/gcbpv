<%= link_to public_send(@path, @workshop), class: "card" do %>
  <div class="card-info">
    <div class="tag-list">
      <% @workshop.teachers.each do |teacher| %>
        <span class="tag" data-tag-color="light">
          <span><%= teacher.name %></span>
        </span>
      <% end %>
      <span class="tag" data-tag-color="dark">
        <span><%= @workshop.city.name %></span>
      </span>
    </div>
    <h3><%= @workshop.name %></h3>
  </div>
<% end %>
