<%= link_to ecole_instrument_path(@instrument), class: "card" do %>
  <div class="card-info">
    <div class="tag-list">
      <% @instrument.cities.each do |city| %>
        <span class="tag" data-tag-color="dark">
          <span><%= city.name %></span>
        </span>
      <% end %>
    </div>
    <h3><%= @instrument.name %></h3>
  </div>
<% end %>
